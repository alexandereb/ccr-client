<!-- Intro Header -->
<header class="intro">
  <div class="intro-body">
    <div class="container">
      <div class="row">
        <div class="col-md-8 col-md-offset-2">
          <h1 class="brand-heading">{{mainInfo.title}}</h1>
          <p class="intro-text">{{mainInfo.calvaryChapel}}</p>
          <a href="" class="btn btn-default btn-lg"><i class="fa fa-video-camera"></i> {{mainInfo.link}}</a>
        </div>
      </div>
    </div>
  </div>
</header>

<!-- Regular Information Section -->
<section id="welcome" class="container content-section text-center">
  <div class="row">
    <div class="col-lg-9 col-lg-offset-2">
      <h2>{{RegularInfo.title}}</h2>
      {{#each RegularInfo.description}}
        <p>{{info}}</p>
      {{/each}}
    </div>
  </div>
</section>

<!-- Download Section -->
<section class="container content-section text-center">
  <div class="row">
    <div class="col-lg-9 col-lg-offset-2">
      <h2>{{Start.title}}</h2>
    </div>
    {{#each Start.list}}
    <div class="col-sm-3 col-xs-6 box-info">
        <strong>{{title}}</strong>
        <div class="box-info-detail">
          <p>{{info}}</p>
        </div>
        <a href="{{link}}">
            <img class="img-responsive portfolio-item" src="http://placehold.it/500x300" alt="">
        </a>
    </div>
    {{/each}}
  </div>
</section>
